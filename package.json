{
  "name": "ssr-bolierplate",
  "version": "1.0.0",
  "description": "Server Side Rendering template with Next JS, React JS, Redux, JSS and TypeScript.",
  "main": "index.js",
  "repository": "https://github.com/wolframdeus/ssr-boilterplate.git",
  "author": "Wolfram Deus <wolfram.deus@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development DEPLOY_ENV=development nodemon ./src/server/index.ts",
    "lint": "tslint ./src/**/*.* --config ./tslint.json --project ./tsconfig.json",
    "lint-fix": "tslint ./src/**/*.* --fix --config ./tslint.json --project ./tsconfig.json",
    "build": "cross-env NODE_ENV=production DEPLOY_ENV=production next build ./src && tsc --project tsconfig.server.json",
    "start": "cross-env NODE_ENV=production DEPLOY_ENV=production node .next/production-server/server/index.js"
  },
  "lint-staged": {
    "linters": {
      "*.(ts|tsx)": [
        "yarn run lint-fix",
        "git add"
      ]
    },
    "ignore": [
      "*.d.ts",
      "**/*.test.tsx"
    ]
  },
  "dependencies": {
    "@date-io/luxon": "^1.3.6",
    "@material-ui/core": "^4.1.1",
    "@material-ui/icons": "^4.2.0",
    "@material-ui/styles": "^4.1.1",
    "@types/jss": "^9.5.8",
    "@types/react-jss": "^8.6.3",
    "@zeit/next-less": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "antd": "^3.19.3",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "colors": "^1.3.3",
    "cookie-parser": "^1.4.4",
    "cross-env": "^5.2.0",
    "dayjs": "^1.8.14",
    "dotenv": "^8.0.0",
    "express": "^4.17.3",
    "express-serve-static-core": "^0.1.1",
    "jss": "^9.8.7",
    "less": "^3.9.0",
    "mobile-detect": "^1.4.3",
    "next": "^8.1.0",
    "next-routes": "^1.4.2",
    "nodemon": "^1.19.1",
    "path-to-regexp": "^3.0.0",
    "pouchdb": "^7.1.1",
    "pouchdb-adapter-memory": "^7.1.1",
    "pouchdb-find": "^7.1.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "styled-jsx": "^3.2.1",
    "ts-node": "^8.2.0",
    "unionize": "^2.2.0",
    "uuid": "^3.3.2",
    "wnumb": "^1.1.0"
  },
  "devDependencies": {
    "@types/colors": "^1.2.1",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.17.0",
    "@types/express-serve-static-core": "^4.16.7",
    "@types/jest": "^24.0.15",
    "@types/luxon": "^1.15.1",
    "@types/mobile-detect": "^1.3.4",
    "@types/next": "^8.0.5",
    "@types/node": "^12.0.8",
    "@types/pouchdb": "^6.3.3",
    "@types/react-redux": "^7.1.0",
    "@types/styled-jsx": "^2.2.8",
    "husky": "^2.4.1",
    "jest": "^24.8.0",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "typescript": "^3.5.2"
  },
  "resolutions": {
    "terser": "3.14.*"
  }
}
